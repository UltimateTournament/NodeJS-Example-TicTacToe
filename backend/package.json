    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/cors": "^2.8.10",
    "@types/node": "^16.0.0",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "standard-version": "^9.2.0",
